<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Core-Regression" parallel="methods" thread-count="10">
	<listeners>
		<listener class-name="com.orasi.utils.listeners.TestListener" />
	</listeners>
	<parameter name="browserUnderTest" value="chrome" />
	<parameter name="environment" value="staging" />
	<parameter name="runLocation" value="local" />
	<parameter name="browserVersion" value="" />
	<parameter name="operatingSystem" value="windows 10" />
 
	<test name="Textbox" parallel="methods" thread-count="20">
		<classes>
			<class name="com.orasi.core.interfaces.TestWebtable" />
		</classes>
	</test>
</suite> <!-- Suite -->
